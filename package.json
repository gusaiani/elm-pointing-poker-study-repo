    "font-awesome": "4.6.3",
    "node-sass": "3.10.0",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.1",
    "webpack-dev-server": "1.16.1"
